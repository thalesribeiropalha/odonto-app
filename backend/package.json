{
  "name": "odonto-app-backend",
  "version": "1.1.0",
  "description": "Backend para sistema de gestão odontológica",
  "main": "server.js",
  "scripts": {
    "start": "npm run build:frontend && node server.js",
    "dev": "node server.js",
    "dev-clean": "node scripts/clear-port.js && node server.js",
    "dev:unified": "npm run build:frontend && node server.js",
    "dev:unified-clean": "node scripts/clear-port.js && npm run build:frontend && node server.js",
    "clean-port": "node scripts/clear-port.js",
    "build:frontend": "cd ../frontend && npm run build",
    "serve": "npm run dev:unified",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "odontologia",
    "gestão",
    "api",
    "nodejs"
  ],
  "author": "Thales Ribeiro",
  "license": "ISC",
  "dependencies": {
    "@supabase/supabase-js": "^2.50.2",
    "axios": "^1.10.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}



